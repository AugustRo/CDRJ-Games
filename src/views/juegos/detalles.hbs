{{#each game}}
<div class="container-lg">
            <div class="juegos__details__content">
                <div class="row">
                    <div class="col-lg-3">
                        <div class="juegos__details__pic set-bg img-fluid" data-setbg="/catalogo/{{img_url}}">
                            <div class="comment"><i class="fa fa-comments"></i> 11</div>
                            <div class="view"><i class="fa fa-eye"></i> 9141</div>
                        </div>
                    </div>
                    <div class="col-lg-9">
                        <div class="juegos__details__text">
                            <div >
                                <h4>{{nombre}}</h3>
                                <span>Nintendo Switch</span>
                            </div>
                            <div class="juegos__details__rating">
                                 <div class="rate">
                                    <input type="radio" id="star5" name="rate" value="5" />
                                    <label for="star5" title="text">5 stars</label>
                                    <input type="radio" id="star4" name="rate" value="4" />
                                    <label for="star4" title="text">4 stars</label>
                                    <input type="radio" id="star3" name="rate" value="3" />
                                    <label for="star3" title="text">3 stars</label>
                                    <input type="radio" id="star2" name="rate" value="2" />
                                    <label for="star2" title="text">2 stars</label>
                                    <input type="radio" id="star1" name="rate" value="1" />
                                    <label for="star1" title="text">1 star</label>
                                </div>
                                <span class=>Valorar</span>
                            </div>
                            <p>{{descripcoin}}.</p>
                            <div class="juegos__details__widget">
                                <div class="row">
                                    <div class="col-lg-10 col-md-7">
                                        <ul>
                                            <li><span>Lanzamiento:</span> 03 marzo 2017</li>
                                            <li><span>Jugadores:</span> Un jugador</li>
                                            <li><span>Género:</span> Aventura, Otros, Acción, Juego de rol</li>
                                            <li><span>Editor:</span> Nintendo</li>
                                            <li><span>Idiomas:</span> Japonés, Inglés, Francés, Alemán, Italiano, Español, Neerlandés, Ruso</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="juegos__details__btn">
                                <a href="#" class="card-link"><button type="button" class="btn btn-info"><i class="fas fa-plus"></i>  Agregar a lista</button></a>
                                <a href="#" class="card-link"><button type="button" class="btn btn-danger"><i class="fas fa-plus"></i>  Mirar video</button></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}

